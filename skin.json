			"+mediawiki.action.view.postEdit": "resources/skinStyles/mediawiki.action.view.postEdit.css",
			"+mediawiki.diff.styles": "resources/skinStyles/mediawiki.diff.styles.css",
			"+mediawiki.htmlform.ooui.styles": "resources/skinStyles/mediawiki.htmlform.ooui.styles.css",
			"+mediawiki.special.userlogin.login.styles": "resources/skinStyles/mediawiki.special.userlogin.login.styles.css",
			"mediawiki.ui": [],
			"mediawiki.ui.checkbox": [],
			"mediawiki.ui.anchor": [],
			"mediawiki.ui.button": [],
			"mediawiki.ui.input": [],
			"mediawiki.ui.radio": [],
			"mediawiki.ui.icon": [],
			"mediawiki.ui.text": []
		}
