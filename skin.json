	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.40.0"
	},
	"ValidSkinNames": {
		"mask": {
			"displayname": "Mask",
			"class": "SkinMask",
			"args": [ {
				"name": "mask",
				"bodyOnly": true,
				"styles": [
					"skins.mask"
				] }
			]
		}
	},
	"MessagesDirs": {
		"SkinMask": [
			"/i18n"
		]
	},
	"AutoloadClasses": {
		"SkinMask": "includes/SkinMask.php",
		"MaskTemplate": "includes/MaskTemplate.php"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Mask"
	},
	"ResourceModules": {
		"skins.mask": {
			"class": "MediaWiki\\ResourceLoader\\SkinModule",
			"features": {
				"content-links": true,
				"content-links-external": true
			},
			"styles": {
				"resources/normalise.css": {
					"media": "screen"
				},
				"resources/fonts.css": {
					"media": "screen"
				},
				"resources/main.css": {
					"media": "screen"
				},
				"resources/print.css": {
					"media": "print"
				}
			}
		}
	},
	"ResourceModuleSkinStyles": {
		"mask": {
			"+ext.echo.styles.badge": "resources/skinStyles/ext.echo.styles.badge.css",
			"+ext.echo.ui": "resources/skinStyles/ext.echo.ui.css",
